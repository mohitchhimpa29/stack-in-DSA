#include<iostream>
using namespace std;

class stack{
    public:
    int *arr;
    int top;
    int size;

    stack(int size){ 
        this->size=size;
        arr=new int[size];
        top=-1;
    }

    void push(int val){
        if(top==(size-1)){
            cout << "overflow " << endl;
        }
        else{
            top++;
            arr[top]=val;
        }
    }

    void pop(){
        if(top==-1){
            cout << "underflow " << endl;
        }
        else{
            top--;
        }
    }

    int peek(){
        if(top==-1){
            cout << "stack is empty " << endl;
            return -1;
        }
        else{
      return arr[top] ;
        }
    }

    void isempty(){
        if(top==-1){
            cout << "stack is empty" << endl;
        }
        else{
            cout << "stack is not empty" << endl;
        }
    }

    
};
int main()
{
    stack st(6);
    st.push(66);
    st.push(54);
    st.push(27);
    st.push(52);
    st.pop();
    cout  <<st.peek() << endl ;
    st.isempty();
    st.pop();
    cout  <<st.peek() << endl ;
    st.isempty();
    st.pop();
    cout  <<st.peek() << endl ;
    st.isempty();
    st.pop();
    cout  <<st.peek() << endl ;
    st.isempty();
    st.pop();
     cout <<st.peek() << endl ;

return 0;
}
